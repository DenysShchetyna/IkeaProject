<hdevelop file_version="1.2" halcon_version="18.11.2.0">
<procedure name="main">
<interface/>
<body>
<l>dev_close_window()</l>
<l>dev_open_window(0, 0, 512, 512, 'black', windowHandle)</l>
<l>set_window_param(windowHandle, 'flush', 'false')</l>
<l>gen_empty_obj(ImgBuffer)</l>
<l>cameraName := 'Cam1LsTopL'</l>
<c></c>
<c></c>
<c></c>
<c></c>
<l>OpenFramegrabber (cameraName, AcqHandle)</l>
<l>grab_image_async(image, AcqHandle, -1)</l>
<c></c>
<l>while(true)</l>
<c>   </c>
<l>    grab_image_async(image, AcqHandle, -1)</l>
<l>    dev_display(image)</l>
<l>    flush_buffer(windowHandle)</l>
<c>    </c>
<l>endwhile</l>
<c></c>
<l>parameter := ''</l>
<l>value := 0</l>
<l>SetFramegrabberParameter (AcqHandle, parameter, value)</l>
<c></c>
<l>GetFramegrabberParameter (AcqHandle, parameter, value)</l>
<c></c>
<l>CloseFramegrabber (AcqHandle) </l>
<c></c>
<c></c>
</body>
<docu id="main">
<parameters/>
</docu>
</procedure>
